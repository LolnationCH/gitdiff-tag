{
  "name": "gitdiff-tag",
  "displayName": "gitdiff-tag",
  "description": "Shows all files changed since the last tag accesible in the current branch",
  "version": "1.5.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LolnationCH/gitdiff-tag"
  },
  "author": "LolnationCH",
  "publisher": "LolnationCH",
  "engines": {
    "vscode": "^1.76.0"
  },
  "icon": "./images/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitdiff-tag.openFile",
        "title": "Open file",
        "icon": "$(go-to-file)"
      },
      {
        "command": "gitdiff-tag.openChanges",
        "title": "Open Changes",
        "icon": "$(go-to-file)"
      },
      {
        "command": "gitdiff-tag.viewTagFile",
        "title": "View file at tag state",
        "icon": "$(go-to-file)"
      },
      {
        "command": "gitdiff-tag.clearCache",
        "title": "Clear Cache",
        "icon": "$(notebook-delete-cell)"
      },
      {
        "command": "gitdiff-tag.listFiles",
        "title": "GitDiff-tag: List Files changed since last tag",
        "icon": "$(list-flat)"
      },
      {
        "command": "gitdiff-tag.refresh",
        "title": "Refresh list of files modified",
        "icon": "$(extensions-refresh)"
      },
      {
        "command": "gitdiff-tag.toggleTree",
        "title": "Toggle tree",
        "icon": "$(testing-show-as-list-icon)"
      }
    ],
    "views": {
      "scm": [
        {
          "id": "gitdiff-tag",
          "name": "GitDiff - Diff from tag"
        }
      ]
    },
    "menus": {
      "commandPalette": [
        {
          "command": "gitdiff-tag.openFile",
          "when": "false"
        },
        {
          "command": "gitdiff-tag.openChanges",
          "when": "false"
        },
        {
          "command": "gitdiff-tag.refresh",
          "when": "false"
        },
        {
          "command": "gitdiff-tag.toggleTree",
          "when": "false"
        }
      ],
      "view/title": [
        {
          "command": "gitdiff-tag.refresh",
          "when": "view == gitdiff-tag",
          "group": "navigation",
          "icon": "$(extensions-refresh)"
        },
        {
          "command": "gitdiff-tag.clearCache",
          "when": "view == gitdiff-tag",
          "group": "navigation",
          "icon": "$(notebook-delete-cell)"
        },
        {
          "command": "gitdiff-tag.toggleTree",
          "when": "view == gitdiff-tag",
          "group": "navigation",
          "icon": "$(testing-show-as-list-icon)"
        }
      ],
      "view/item/context": [
        {
          "command": "gitdiff-tag.openFile",
          "when": "view == gitdiff-tag",
          "group": "navigation"
        },
        {
          "command": "gitdiff-tag.openChanges",
          "when": "view == gitdiff-tag",
          "group": "3_compare"
        },
        {
          "command": "gitdiff-tag.viewTagFile",
          "when": "view == gitdiff-tag",
          "group": "3_compare"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "ts-loader": "^9.4.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "@vscode/test-electron": "^2.2.3"
  }
}